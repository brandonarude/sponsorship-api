{
  "kind": "collectionType",
  "collectionName": "Church_notes",
  "info": {
    "name": "Church_note"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "note_id": {
      "type": "string",
      "required": true,
      "unique": true,
      "private": true
    },
    "unique_id": {
      "type": "uid",
      "targetField": "note_id",
      "required": true,
      "private": false
    },
    "description": {
      "type": "text",
      "maxLength": 5000,
      "required": true,
      "private": true
    },
    "church": {
      "via": "church_notes",
      "model": "church"
    }
  }
}
