{
  "kind": "collectionType",
  "collectionName": "Children",
  "info": {
    "name": "child",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "child_id": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 5
    },
    "old_wp_post_id": {
      "type": "string",
      "unique": true,
      "private": true
    },
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": false,
      "private": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "female",
        "male"
      ],
      "required": false
    },
    "description": {
      "type": "text"
    },
    "nick_name": {
      "type": "string",
      "private": true
    },
    "exit_status": {
      "type": "enumeration",
      "enum": [
        "still_active",
        "ninth_grade_certificate",
        "grad_vocational_school",
        "grad_college",
        "college_scholarship",
        "marriage",
        "behavior",
        "family_request",
        "other"
      ],
      "private": true
    },
    "exit_notes": {
      "type": "string",
      "private": true
    },
    "in_undisclosed_country": {
      "type": "boolean",
      "default": false,
      "required": false,
      "private": true
    },
    "total_sponsorships": {
      "type": "integer",
      "min": 0,
      "private": false,
      "default": 0,
      "required": false
    },
    "total_sponsorship_dollars": {
      "type": "integer",
      "required": false,
      "default": 0,
      "private": false,
      "min": 0,
      "unique": false
    },
    "unique_id": {
      "type": "uid",
      "targetField": "child_id",
      "required": true,
      "private": true
    },
    "home": {
      "via": "children",
      "model": "home"
    },
    "child_notes": {
      "via": "child",
      "collection": "child-note"
    },
    "sponsorships": {
      "via": "child",
      "collection": "sponsorship"
    },
    "date_archived": {
      "type": "datetime",
      "default": "1970-01-01T20:00:00.000Z",
      "private": true
    },
    "country": {
      "model": "country",
      "via": "children"
    },
    "publish_status": {
      "type": "enumeration",
      "enum": [
        "archive",
        "auto_draft",
        "draft",
        "inactive",
        "inherit",
        "over_18",
        "pending",
        "private",
        "publish",
        "trash"
      ],
      "default": "publish"
    },
    "wp_post_date_gmt": {
      "type": "datetime",
      "default": "2008-10-13T19:00:00.000Z"
    },
    "birthday": {
      "type": "datetime",
      "default": "1970-01-01T20:00:00.000Z"
    }
  }
}
